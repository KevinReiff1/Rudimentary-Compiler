{ int a a = 5 print(a) }$
{ string b b = "hello" print(b) }$
{ boolean c c = true print(c) }$
{ int a int b a = 5 b = 10 print(a) print(b) }$
{ int a { int a a = 1 print(a) } print(a) }$
{ int a { int b { int c c = 3 print(c) } print(b) } print(a) }$
{ string s s = "a" { s = "b" { s = "c" print(s) } print(s) } print(s) }$
{ int a a = 5 if (a == 5) { print(a) } else { print(0) } }$
{ boolean flag flag = false if (flag != true) { print(flag) } else { print(true) } }$
{ int a a = 0 while (a != 3) { print(a) a = a + 1 } }$
{ int a a = 5 print(a) }$
